@page "/counter"


<PageTitle>Contador</PageTitle>

<h1>Counter</h1>

<p role="status">Current count: @currentCount</p>
<p role="status">Current Singleton: @variableSingleton.Valor</p>
<p role="status">Current Transient: @variableTransient.Valor</p>

<button class="btn btn-primary" @onclick="IncrementCount">Click me</button>

